{
    "name": "a-h-abid/abd-validator",
    "version": "0.0.1",
    "type": "library",
    "description": "Validator Library.",
    "keywords": ["validator", "library"],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "AHAbid\\AbdValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AHAbid\\AbdValidator\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
